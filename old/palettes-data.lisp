(defvar *colors*
  (let ((tmp (make-hash-table)))
    (dolist (i *colors-tmp*)
      (let ((symbols (car i))
            (hashtable (alist-hash-table (cdr i) :test #'equal)))
        (mapc (lambda (x) (setf (gethash x tmp) hashtable)) symbols)))
    tmp))

